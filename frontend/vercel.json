{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "build" // Pastikan sesuai output folder build Anda
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200 // Tambahkan status code
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    }
  ]
}